 <div id="game" ng-controller='ticToeCtrl'>
   <ul id="teams">
    <li ng-repeat='player in players'>
      <img ng-src="">
      <h2>Team <span class="team" >{{player.marker}}</span></h2>
      <h3 class="player">{{player.name}}</h3>
    </li>
  </ul>
  <div id="board" >
    <div  ng-repeat='tile in tiles' id="{{tile}}" class='tile' ng-click="alert()"></div>
  </div>
  <div id="results">
    <h1></h1>
    <span class="image"></span>
    <button>play again</button>
  </div>
</div>
