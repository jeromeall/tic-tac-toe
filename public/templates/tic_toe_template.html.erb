<h3><a href="/#{{game}}">{{gameLink}}</a></h3>

 <div id="game">
   <ul id="teams">
    <li ng-repeat='player in players' ng-class="{current: currentPlayer().indicator === $index}">
      <!-- <img src='app/as{{player.image}}'> -->
      <h2>Team <span class="team" >{{player.marker}}</span></h2>
      <h3 class="player">{{player.name}}</h3>
    </li>
  </ul>
  <div id="board" >
    <div  ng-repeat='tile in tiles' id="{{tile.name}}" class='tile' ng-click="addMarker($index)">{{tile.letter}}</div>
  </div>
  <div id="results" ng-hide="tieOrWin" class="ng-hide">
    <h1 ng-model='message'>{{message}}</h1>
    <span class="image"></span>
    <button ng-click="playAgain()">play again</button>
  </div>
</div>
